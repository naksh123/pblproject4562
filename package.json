{
  "name": "travel-packing-list",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://travel-packing-list.com",
  "engines": {
    "yarn": "1.22.18"
  },
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "prebuild": "yarn flow && yarn lint && yarn test",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:coverage": "yarn test --coverage",
    "flow": "flow --max-warnings=0",
    "lint": "eslint .",
    "lint:fix": "eslint . --quiet --fix",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@kiwicom/orbit-components": "^0.115.0",
    "@kiwicom/orbit-design-tokens": "^0.13.7",
    "html2pdf.js": "^0.10.1",
    "i18next": "^20.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.11.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-orbit-components": "^0.5.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "flow-bin": "^0.153.0",
    "flow-typed": "^3.3.1",
    "gh-pages": "^3.2.2",
    "jest":"26.6.0",
    "prettier": "^2.3.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
